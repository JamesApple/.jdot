{
  "diagnostic.enable": true,
  "snippets.ultisnips.pythonVersion": 3,
  "snippets.ultisnips.usePythonx": false,
  "snippets.autoTrigger": false,
  "snippets.triggerCharacters": [".", "::"],
  // "~/.vim_plugged/ansible-vim/UltiSnips" Manually generate and write these
  "snippets.ultisnips.directories": [
    "~/.vim/UltiSnips"
  ],
  "yaml.trace.server": "verbose",
  "yaml.format.enable": true,
  "yaml.format.bracketSpacing": true,
  "yaml.hover": true,
  "yaml.validate": true,
  "yaml.completion": true,
  "yaml.format.singleQuote": false,
  "yaml.schemas": {
    // "http://json.schemastore.org/ansible-stable-2.9": [  "*" ], Cant use due to invalid schemas...
    "kubernetes": ["*"],
    "https://raw.githubusercontent.com/softprops/serverless-yml-schema/master/serverless-schema.json": "serverless.yml"
  },
  "json.schemas": [
    {
      "url": "http://json.schemastore.org/package",
      "fileMatch": ["package.json"]
    },
    {
      "url": "http://json.schemastore.org/tsconfig",
      "fileMatch": ["*tsconfig*"]
    },
    {
      "url": "http://json.schemastore.org/eslintrc",
      "fileMatch": ["*eslintrc*"]
    }
  ],
  "java.import.maven.enabled": true,
  "java.format.enabled": true,
  "java.format.settings.url": "/Users/jamesapple/.config/coc/extensions/node_modules/redhat.java/eclipse-formatter.xml",
  "java.maven.downloadSources": true,
  "java.completion.enabled": true,
  "java.completion.guessMethodArguments": true,
  "java.completion.overwrite": false,
  "java.progressReports.enabled": true,
  "java.autobuild.enabled": true,
  "java.contentProvider.preferred": "fernflower",
  "javascript.format.enabled": true,
  "java.signatureHelp.enabled": true,
  "java.implementationsCodeLens.enabled": true,
  "java.referencesCodeLens.enabled": true,
  "java.errors.incompleteClasspath.severity": "info",
  "java.configuration.updateBuildConfiguration": "automatic",
  "diagnostic.level": "information",
  "typescript.suggest.paths": true,
  "typescript.showUnused": true,
  "typescript.validate.enable": true,
  "typescript.suggest.autoImports": true,
  "typescript.implementationsCodeLens.enable": true,
  "typescript.suggestionActions.enabled": true,
  "typescript.suggest.enabled": true,
  "coc.preferences.colorSupport": true,
  "highlight.colors.enable": true,
  "highlight.document.enable": true,
  "languageserver": {
  }
}
